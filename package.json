{
  "name": "videojs-nle-controls",
  "version": "0.0.0",
  "description": "Adds frame-by-frame scrubbing and SMPTE timecode to a videojs player instance",
  "scripts": {
    "prebuild": "npm run clean",
    "build:js:browser": "webpack",
    "build:js:babel": "babel src/plugin.js --out-file dist/videojs-nle-controls.js",
    "build:js:min": "babel src/plugin.js --out-file dist/videojs-nle-controls.js && uglifyjs dist/videojs-nle-controls.js -o dist/videojs-nle-controls.min.js",
    "test": "karma start test/karma.conf.js",
    "test:chrome": "npm run pretest && karma start test/karma.conf.js --browsers Chrome",
    "test:firefox": "npm run pretest && karma start test/karma.conf.js --browsers Firefox",
    "test:ie": "npm run pretest && karma start test/karma.conf.js --browsers IE",
    "test:safari": "npm run pretest && karma start test/karma.conf.js --browsers Safari"
  },
  "keywords": [
    "videojs",
    "videojs-plugin"
  ],
  "author": "jjromphf <jromphf@library.rochester.edu>",
  "license": "MIT",
  "style": "dist/videojs-nle-controls.css",
  "videojs-plugin": {
    "style": "dist/videojs-nle-controls.css",
    "script": "dist/videojs-nle-controls.min.js"
  },
  "vjsstandard": {
    "ignore": [
      "dist",
      "docs",
      "es5",
      "test/dist",
      "test/karma.conf.js"
    ]
  },
  "files": [
    "CONTRIBUTING.md",
    "dist/",
    "docs/",
    "index.html",
    "src/",
    "test/"
  ],
  "dependencies": {
    "video.js": "^5.6.0"
  },
  "devDependencies": {
    "babel": "^5.8.35",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^6.4.0",
    "bannerize": "^1.0.2",
    "bluebird": "^3.2.2",
    "budo": "^8.0.4",
    "doctoc": "^0.15.0",
    "glob": "^6.0.3",
    "global": "^4.3.0",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-detect-browsers": "^2.0.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-qunit": "^0.1.9",
    "karma-safari-launcher": "^0.1.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^1.5.1",
    "qunitjs": "^1.21.0",
    "rimraf": "^2.5.1",
    "sinon": "~1.14.0",
    "uglify-js": "^2.6.1",
    "videojs-standard": "^4.0.0",
    "webpack": "^1.13.1"
  }
}
